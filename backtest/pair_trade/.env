## MT5账户信息
MT5_ACCOUNT=123456
MT5_SERVER=MT5_SERVER
MT5_PASSWORD=88888888

## 配对交易品种与统计窗口
PAIR_TRADE_CATEGORY_A=EURUSD
PAIR_TRADE_CATEGORY_B=GBPUSD
PAIR_TRADE_N_DAYS=5

## 回测区间（UTC）
BACKTEST_START_TIME=2026-01-01 00:00
BACKTEST_END_TIME=2026-02-01 00:00

## 信号参数
BACKTEST_SIGNAL_TIMEFRAME=M1
BACKTEST_TRADING_TIMEFRAME=S1
BACKTEST_ADF_THRESHOLD=0.1
BACKTEST_ENTRY_ZSCORE=1.6
BACKTEST_EXIT_ZSCORE=0.8
BACKTEST_SKIP_WEEKEND=true

## 资金参数
BACKTEST_PRINCIPAL=10000
BACKTEST_TOTAL_LOT=0.1

## 成本与合约（对应品种A/B）
BACKTEST_TRADING_COST_A=5
BACKTEST_TRADING_COST_B=5
BACKTEST_CONTRACT_SIZE_A=100000
BACKTEST_CONTRACT_SIZE_B=100000

## 夏普收益重采样频率（默认日频）
BACKTEST_RESAMPLE_RULE=D

## 输出目录
BACKTEST_OUTPUT_DIR=./result
